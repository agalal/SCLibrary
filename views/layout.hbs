          <g class="g--circle">
            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#s--circle" class="u--circle"></use>
          </g>
          <g class="g--circle">
            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#s--circle" class="u--circle"></use>
          </g>
        </g>
    </svg>
</div>


